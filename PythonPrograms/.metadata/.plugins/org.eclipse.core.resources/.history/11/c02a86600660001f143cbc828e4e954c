def getTable():
    print ("hi")
#    for i in range(1,1):
    response=urllib2.urlopen("http://www.soundkeepers.com/GRE/print.php?n="+str(1)).read()
    p=TableParser()
    
    p.feed(response)
#       create a file that will hold the content 
    file=open("file"+str(1)+".txt","w")
